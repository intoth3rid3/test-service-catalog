{
    "compilerOptions": {
        "target": "ES6",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress"
        ],
        "esModuleInterop": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts"
    ]
}